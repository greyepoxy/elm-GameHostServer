{
  "name": "elm-gamehostserver",
  "version": "0.0.1",
  "description": "Server to host a game, partially written in elm.",
  "main": "dist/server.main.js",
  "scripts": {
    "build": "rimraf dist && webpack --config webpack.prod.client.config.js && webpack --config webpack.dev.server.config.js",
    "watchServer": "webpack --config webpack.dev.server.config.js --watch",
    "startDev": "webpack-dev-server --config webpack.dev.client.config.js --port 3010 --hot --inline",
    "startServer": "nodemon ./dist/server.main.js localhost 3000 --watch dist",
    "startDebugServer": "set DEBUG=express:* & npm run start",
    "test": "webpack-dev-server --config webpack.test.client.config.js --port 8000 --inline"
  },
  "keywords": [
    "elm",
    "webpack",
    "elm-test",
    "elm-webpack-loader",
    "elm-hot-loader"
  ],
  "author": "greyepoxy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/greyepoxy/elm-GameHostServer.git"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "basscss": "^8.0.1",
    "css-loader": "^0.23.1",
    "elm-hot-loader": "^0.3.2",
    "elm-webpack-loader": "^3.0.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.17.0",
    "nodemon": "^1.9.2",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "virtual-dom": "^2.1.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.13.0"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "express": "^4.13.4"
  }
}
